{
  "name": "[INTERNAL]DOOH Tweets Curator",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Internal extension used to select tweet to be featured onto the display",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://twitter.com/*", "storage", "tabs"
  ],
  "content_scripts": [{
    "matches": ["https://twitter.com/*"],
    "js": ["jquery-3.4.1.min.js", "content.js"]
  }]
}